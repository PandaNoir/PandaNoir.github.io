{
  "name": "pandanoir.github.io",
  "version": "0.0.1",
  "description": "[pandanoir.github.io](http://pandanoir.github.io)",
  "main": "lazy-content.js",
  "scripts": {
    "prebuild": "mkdirp ./public",
    "build": "run-p build:html build:css",
    "build:html": "pug < ./src/index.pug | tr -d '\n' > ./public/index.html",
    "build:css": "node-sass ./src/style.scss ./public/style.css --output-style compressed"
  },
  "keywords": [],
  "author": "Naoto Ikuno <moon_in_france@yahoo.co.jp>",
  "license": "MIT",
  "dependencies": {
    "mkdirp": "^1.0.4",
    "node-sass": "^4.14.1",
    "pug": "^2.0.4",
    "pug-cli": "^1.0.0-alpha6"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
